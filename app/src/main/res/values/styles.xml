<resources>

    <!-- Base application theme. -->
    <style name="AppThemeLight" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorLightPrimaryStyles</item>
        <item name="colorPrimaryDark">@color/colorLightPrimaryDarkStyles</item>
        <item name="colorAccent">@color/colorLightAccentStyles</item>

        <item name="colorControlNormal">@color/colorLightControlNormalStyles</item>
        <item name="colorControlActivated">@color/colorLightPrimaryStyles</item>
    </style>

    <style name="AppThemeDark" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorDarkPrimaryStyles</item>
        <item name="colorPrimaryDark">@color/colorDarkPrimaryDarkStyles</item>
        <item name="colorAccent">@color/colorDarkAccentStyles</item>

        <item name="colorControlNormal">@color/colorDarkControlNormalStyles</item>
        <item name="colorControlActivated">@color/colorDarkPrimaryStyles</item>
    </style>


    <style name="AuthUIThemeLight" parent="AppThemeLight">
        <item name="colorPrimary">@color/colorLightPrimaryAuthUITheme</item>
        <item name="colorPrimaryDark">@color/colorLightPrimaryDarkAuthUITheme</item>
        <item name="colorAccent">@color/colorLightAccentAuthUITheme</item>
        <item name="colorSecondary">@color/colorLightSecondaryAuthUITheme</item>
        <!--        <item name="android:buttonStyle">@style/buttonStyle</item>-->
    </style>

    <style name="AuthUIThemeDark" parent="AppThemeDark">
        <item name="colorPrimary">@color/colorDarkPrimaryAuthUITheme</item>
        <item name="colorPrimaryDark">@color/colorDarkPrimaryDarkAuthUITheme</item>
        <item name="colorAccent">@color/colorDarkAccentAuthUITheme</item>
        <item name="colorSecondary">@color/colorDarkSecondaryAuthUITheme</item>
        <!--        <item name="android:buttonStyle">@style/buttonStyle</item>-->
    </style>

    <style name="ToolBarStyle" parent="">
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
        <item name="android:theme">@style/ThemeOverlay.AppCompat.Dark</item>
    </style>

    <style name="ToolbarTitle" parent="@style/TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="roundedCorners" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">6dp</item>
    </style>


    <style name="buttonStyleLight" parent="MaterialTextButtonLight">
        <item name="android:textColor">@color/colorLightTextButtonStyle</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="buttonStyleDark" parent="MaterialTextButtonDark">
        <item name="android:textColor">@color/colorDarkTextButtonStyle</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="MaterialTextButtonLight" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:textAppearance">@style/Text16.Light</item>
        <item name="android:textColor">@color/colorLightTextMaterialTextButtonStyle</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="MaterialTextButtonDark" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:textAppearance">@style/Text16.Light</item>
        <item name="android:textColor">@color/colorDarkTextMaterialTextButtonStyle</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="DialogThemeLight" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:background">@color/colorLightBackgroundDialogTheme</item>
        <item name="android:textColor">@color/colorLightTextDialogTheme</item>
    </style>

    <style name="DialogThemeDark" parent="ThemeOverlay.AppCompat.Dialog.Alert">
        <item name="android:background">@color/colorDarkBackgroundDialogTheme</item>
        <item name="android:textColor">@color/colorDarkTextDialogTheme</item>
    </style>

    <style name="DialogStyle" parent="Theme.MaterialComponents.Light.BottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <!--        <item name="android:statusBarColor">@android:color/transparent</item>-->
        <item name="android:windowSoftInputMode">adjustResize</item>
    </style>


    <style name="SpinnerDropDownItemStyle" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:textAppearance">@style/Text14.Medium</item>
    </style>

    <style name="SpinnerStyle">
        <item name="android:showDividers">none</item>
        <item name="android:dropDownHeight">70dp</item>
        <item name="android:dropDownSelector">@android:color/white</item>
    </style>

    <style name="CalenderViewWeekCustomText" parent="Text16.Medium.Primary" />

